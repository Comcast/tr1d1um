{
	"fqdn": "fake-fqdn",
	
	"primary": {
		"address": ":443",
	},

	"alternate": {
		"address": ":8090",
	},

	"health": {
		"address": ":80",
		"logInterval": "60s",
		"options": [
			"PayloadsOverZero",
			"PayloadsOverHundred",
			"PayloadsOverThousand",
			"PayloadsOverTenThousand"
		]
	},

	"pprof": {
		"address": ":9999"
	},
	
	"metrics": {
		"address": ":8080"
	},
	
	"log": {
		"file"      : "/var/log/tr1d1um/tr1d1um.log",
		"level"     : "DEBUG",
		"maxSize"   : 52428800,
		"maxBackup" : 10,
		"json"      : true
	},

	"aws": {
		"accessKey": "fake-accessKey",
		"secretKey": "fake-secretKey",
		"env": "fake-env",
		"sns": {
			"region": "fake region",
			"topicArn": "fake-sns-topic",
			"urlPath" : "/api/v2/aws/sns"
		}
	},

	"targetURL": "localhost:6000",
	"supportedServices": ["config"]
}
